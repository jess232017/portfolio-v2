{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "react/react-in-jsx-scope": "off"
  },
  "plugins": ["react"],
  "extends": ["next/core-web-vitals", "plugin:react/recommended", "prettier", "plugin:jsx-a11y/recommended"]
}
